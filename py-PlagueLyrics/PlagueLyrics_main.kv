<ContainerBox>:
        FloatLayout:
                GridLayout:
                        id: MenuBar
                        size_hint: 1, 0.04
                        pos_hint:{"x":0, "y":.96}
                        cols: 8
                        orientation: 'horizontal'
                        Button:
                                text: 'Open'
                                on_release: root.show_load()
                        Button:
                                text: 'Save As'
                                on_release: root.save_project_as()
                        Button:
                                text: 'Save'
                                on_release: root.try_save()
                        Button:
                                text: 'Load Video'
                                on_release: root.try_video()
                        Button:
                                text: 'Export .srt'
                                on_release: root.try_export()
                        Button:
                                text: 'Hard-subs'
                                on_release: root.TestFunc()
                        Button:
                                text: 'Exit'
                                on_release: root.close_app()
                GridLayout:
                        id: SelectPartition
                        size_hint: .3, 0.05
                        pos_hint:{"x":0, "y":.9}
                        cols: 8
                        orientation: 'horizontal'
                        Button:
                                text: '<'
                                on_release: root.PrevPartition()
                        Label:
                                id: lbl_selected
                                size_hint: 3, .05
                                text: "0"
                        Button:
                                text: '>'
                                on_release: root.NextPartition()
                BoxLayout:
                        id: LyricsList
                        size_hint: .3, .9
                        pos_hint:{"x":0, "y":0}
                        orientation: 'vertical'
                        TextInput:
                                id: Lyrics
                                background_color: [.5,.5,.5,.5]
                                on_text: root.Generate(0)
                Label:
                        id: lbl_msg
                        size_hint: .35, 0.05
                        pos_hint:{"x":.3, "y":.9}
                BoxLayout:
                        id: LyricsOutList
                        size_hint: .35, .9
                        pos_hint:{"x":.3, "y":0}
                        orientation: 'vertical'
                        TextInput:
                                id: LyricsOutput
                                background_color: [.5,.5,.5,.5] 
                GridLayout:
