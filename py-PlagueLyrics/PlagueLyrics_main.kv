<ContainerBox>:
        FloatLayout:
                GridLayout:
                        id: MenuBar
                        size_hint: 1, 0.04
                        pos_hint:{"x":0, "y":.96}
                        cols: 8
                        orientation: 'horizontal'
                        Button:
                                text: 'Open'
                                on_release: root.load()
                        Button:
                                text: 'Save As'
                                on_release: root.save(1)
                        Button:
                                text: 'Save'
                                on_release: root.save(0)
                        Button:
                                text: 'Load Audio'
                                on_release: root.loadAudio()
                        Button:
                                text: 'Export .srt'
                                on_release: root.try_export()
                        Button:
                                text: 'Hard-subs'
                        Button:
                                text: 'Exit'
                                on_release: root.close_app()
                GridLayout:
                        id: SelectPartition
                        size_hint: .3, 0.05
                        pos_hint:{"x":0, "y":.79}
                        cols: 8
                        orientation: 'horizontal'
                        Button:
                                text: '<'
                                on_release: root.PrevPartition()
                        Label:
                                id: lbl_selected
                                size_hint: 3, .05
                                text: "0"
                        Button:
                                text: '>'
                                on_release: root.NextPartition()
                Label:
                        id: lbl_msg
                        size_hint: .35, 0.05
                        pos_hint:{"x":.3, "y":.79}
                        font_size: 18
                Label:
                        id: lbl_subs
                        size_hint: .66, .12
                        pos_hint:{"x":0, "y":.84}
                        font_size: 30
                BoxLayout:
                        id: LyricsList
                        size_hint: .3, .79
                        pos_hint:{"x":0, "y":0}
                        orientation: 'vertical'
                        TextInput:
                                id: Lyrics
                                background_color: [.6,.6,.6,.5]
                                #foreground_color: [1,0,0,1]
                                on_text: root.Generate(0)
                BoxLayout:
                        id: LyricsOutList
                        size_hint: .35, .79
                        pos_hint:{"x":.3, "y":0}
                        orientation: 'vertical'
                        TextInput:
                                id: LyricsOutput
                                background_color: [.6,.6,.6,.5]
                                #foreground_color: [0,1,0,1]
                GridLayout:
                        id: LyricsSize
                        size_hint: .06, .03
                        pos_hint:{"x":.24, "y":.75}
                        cols: 2
                        orientation: 'horizontal'
                        Button:
                                text: '-'
                                on_release: root.LyricsSize(0)
                        Button:
                                text: '+'
                                on_release: root.LyricsSize(1)
                GridLayout:
                        id: SubSize
                        size_hint: .06, .03
                        pos_hint:{"x":.6, "y":.92}
                        cols: 2
                        orientation: 'horizontal'
                        Button:
                                text: '-'
                                on_release: root.SubSize(0)
                        Button:
                                text: '+'
                                on_release: root.SubSize(1)
                GridLayout:
                        id: Settings
                        size_hint: .33, .46
                        pos_hint:{"x":.66, "y":0}
                        rows: 9
                        orientation: 'vertical'
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        text: " "
                                Label:
                                        text: "Start(ms):"
                                Label:
                                        text: "BPM:"
                                Label:
                                        text: "Beats:"
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_0
                                        text: "1"
                                TextInput:
                                        id: inp_start0
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm0
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat0
                                        text: "4"
                                        on_text: root.Generate(0)               
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_1
                                        text: "2"
                                TextInput:
                                        id: inp_start1
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm1
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat1
                                        text: "4"
                                        on_text: root.Generate(0)
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_2
                                        text: "3"
                                TextInput:
                                        id: inp_start2
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm2
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat2
                                        text: "4"
                                        on_text: root.Generate(0)
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_3
                                        text: "4"
                                TextInput:
                                        id: inp_start3
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm3
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat3
                                        text: "4"
                                        on_text: root.Generate(0)
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_4
                                        text: "5"
                                TextInput:
                                        id: inp_start4
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm4
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat4
                                        text: "4"
                                        on_text: root.Generate(0)
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_5
                                        text: "6"
                                TextInput:
                                        id: inp_start5
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm5
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat5
                                        text: "4"
                                        on_text: root.Generate(0)
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_6
                                        text: "7"
                                TextInput:
                                        id: inp_start6
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm6
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat6
                                        text: "4"
                                        on_text: root.Generate(0)
                        GridLayout:
                                cols: 4
                                orientation: 'horizontal'
                                Label:
                                        id: lbl_7
                                        text: "8"
                                TextInput:
                                        id: inp_start7
                                        text: "0"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_bpm7
                                        text: "120"
                                        on_text: root.Generate(0)
                                TextInput:
                                        id: inp_beat7
                                        text: "4"
                                        on_text: root.Generate(0)
                GridLayout:
                        id: AudioButtons
                        size_hint: .33, .05
                        pos_hint:{"x":.66, "y":.47}
                        cols: 8
                        orientation: 'horizontal'
                        Button:
                                text: 'Play'
                                on_release: root.PlayAudio()
                        Button:
                                text: 'Stop'
                                on_release: root.StopAudio()
                        Button:
                                text: 'Vol Up'
                                on_release: root.AudioVolume(1)
                        Button:
                                text: 'Vol Down'
                                on_release: root.AudioVolume(0)
                Slider:
                        id: AudioSlider
                        min: 0
                        max: 1
                        value: 0
                        size_hint: .34, .04
                        pos_hint:{"x":.66, "y":.52}
                        on_touch_up: root.ChangeAudioPos()
