                GridLayout:
                        id: VideoButtons
                        size_hint: .33, .05
                        pos_hint:{"x":.66, "y":.51}
                        cols: 8
                        orientation: 'horizontal'
                        Button:
                                text: 'Play'
                                on_release: root.PlayVideo()
                        Button:
                                text: 'Stop'
                                on_release: root.StopVideo()
                        Button:
                                text: 'Vol Up'
                        Button:
                                text: 'Vol Down'
                Video:
                        id: VideoPlayer
                        size_hint: .34, .4
                        pos_hint:{"x":.66, "y":.56}
                        state: 'pause'

<LoadVideoDialog>:
        BoxLayout:
                size: root.size
                pos: root.pos
                orientation: "vertical"
                FileChooserListView:
                        id: filechooser

                BoxLayout:
                        size_hint_y: None
                        height: 30
                        Button:
                                text: "Cancel"
                                on_release: root.cancel()

                        Button:
                                text: "Load"
                                on_release: root.loadvideo(filechooser.path, filechooser.selection[0])

<LoadDialog>:
        BoxLayout:
                size: root.size
                pos: root.pos
                orientation: "vertical"
                FileChooserListView:
                        id: filechooser

                BoxLayout:
                        size_hint_y: None
                        height: 30
                        Button:
                                text: "Cancel"
                                on_release: root.cancel()

                        Button:
                                text: "Load"
                                on_release: root.load(filechooser.path, filechooser.selection)

<SaveDialog>:
        text_input: text_input
        BoxLayout:
                size: root.size
                pos: root.pos
                orientation: "vertical"
                FileChooserListView:
                        id: filechooser
                        on_selection: text_input.text = self.selection and self.selection[0] or ''

                TextInput:
                        id: text_input
                        size_hint_y: None
                        height: 30
                        multiline: False

                BoxLayout:
                        size_hint_y: None
                        height: 30
                        Button:
                                text: "Cancel"
                                on_release: root.cancel()

                        Button:
                                text: "Save"
                                on_release: root.save(filechooser.path, text_input.text)
